class Pessoa{
	string nome;
	int idade;

	void setNome(string nome_){
		nome = nome_;
	}

	void setIdade(int idade_){
		idade = idade_;
	}

	void setPessoa(string nome_, int idade_){
		nome = nome_;
		idade = idade_;
	}
}

void main(){

	int idade;
	string nome;
	Pessoa p;

	print("Digite seu nome: ");
	nome = readLine();
	print("Digite sua idade: ");
	idade = readInteger();
	p.setPessoa(nome, idade);
	print("Oi ", p.nome, " vocÃª tem ", p.idade, " anos.");
}